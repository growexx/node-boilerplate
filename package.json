{
  "name": "growexx-node-boiler-plate",
  "version": "0.1.0",
  "description": "Growexx node boiler plate for API",
  "main": "index.js",
  "scripts": {
    "prestart": "NODE_ENV=local node_modules/migrate-mongo/bin/migrate-mongo.js up",
    "up": "NODE_ENV=local migrate-mongo up",
    "start": "NODE_ENV=local nodemon .",
    "test": "NODE_ENV=testing nyc mocha test/alltests.js --exit",
    "jsdoc": "./node_modules/.bin/jsdoc server/* -r  --destination jsdocs/jsdocs",
    "commit": "git-cz"
  },
  "author": "Growexx",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.437.0",
    "@aws-sdk/client-ses": "^3.409.0",
    "@aws-sdk/node-http-handler": "^3.374.0",
    "@aws-sdk/s3-request-presigner": "^3.437.0",
    "@aws-sdk/types": "^3.408.0",
    "@aws-sdk/client-secrets-manager": "^3.427.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.1",
    "@semantic-release/npm": "^7.1.3",
    "@semantic-release/release-notes-generator": "^9.0.3",
    "aws-sdk": "^2.1454.0",
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "config": "^3.3.9",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ftp": "^0.3.10",
    "helmet": "^7.0.0",
    "i18n": "^0.8.6",
    "jest": "^29.6.4",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "method-override": "^3.0.0",
    "migrate-mongo": "^10.0.0",
    "moment": "^2.29.4",
    "mongoose": "^7.5.2",
    "mongoose-aggregate-paginate-v2": "^1.0.6",
    "mongoose-paginate-v2": "^1.7.31",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-fetch": "^3.3.2",
    "nodemailer": "^6.9.5",
    "openai": "^4.7.1",
    "randomstring": "^1.3.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0"
  },
  "devDependencies": {
    "chai": "^4.3.8",
    "chai-http": "^4.4.0",
    "cz-conventional-changelog": "^3.3.0",
    "jsdoc": "^4.0.2",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "sinon": "^15.2.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "typescript": "^5.2.2"
  },
  "nyc": {
    "lines": 5,
    "statements": 5,
    "functions": 5,
    "branches": 5,
    "check-coverage": true,
    "exclude": [
      "node_modules",
      "**/test/**",
      "coverage",
      "migrations",
      "jsdocs",
      ".eslintrc.js",
      "migrate-mongo-config.js",
      "util/country.js",
      "util/currency.js",
      "util/timeZone.js",
      "util/languageISO.js",
      "util/http-status.js"
    ],
    "reporter": [
      "lcov",
      "html"
    ],
    "cache": true,
    "all": true
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/Growexx-master/Growexx-api.git"
  },
  "release": {
    "repositoryUrl": "https://bitbucket.org/Growexx-master/Growexx-api.git",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "publish": [
      {
        "path": "@semantic-release/npm",
        "npmPublish": false,
        "tarballDir": "dist"
      }
    ]
  },
  "homepage": "https://bitbucket.org/ZsigDevelopment/semver-demo.git#readme",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
